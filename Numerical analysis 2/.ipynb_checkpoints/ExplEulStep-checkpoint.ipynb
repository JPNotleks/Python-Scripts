    "import numpy as np\n",
    "\n",
    "def ExplEulStep(A, Y0, h):\n",
    "    # explicit Euler step\n",
    "    # Input:\n",
    "    #\tA - matrix\n",
    "    #\tY0 - initial value\n",
    "    #\th - step size\n",
    "    \n",
    "    Y = Y0+h*np.matmul(A,Y0)\n",
    "    \n",
    "    return Y"
   ]
