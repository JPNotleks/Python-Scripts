   "source": [
    "import numpy as np\n",
    "\n",
    "def ImplMidpStep(A, Y0, h):\n",
    "    # step of implicit midpoint rule\n",
    "    # Input:\n",
    "    #\tA - matrix\n",
    "    #\tY0 - initial value\n",
    "    #\th - step size\n",
    "    \n",
    "    Y = np.linalg.inv(np.identity(np.shape(A)[0])-h/2*A)*(Y0+h/2*A*Y0)\n",
    "    \n",
    "    return Y"
   ]
  }
